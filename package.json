{
  "name": "ss-twitter",
  "version": "0.0.0",
  "description": "A NodeCG bundle to display Tweets",
  "dependencies": {
    "numeral": "^1.5.3",
    "q": "^1.4.1",
    "request": "^2.61.0",
    "twitter": "^1.2.5"
  },
  "homepage": "http://splitscreenshow.com/open-source/",
  "author": {
    "name": "David Thomas",
    "email": "guy@synth3.tk",
    "url": "http://synth3.tk"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg",
    "nodecg-bundle",
    "twitter"
  ],
  "nodecg": {
    "compatibleRange": "~0.7.0",
    "dashboardPanels": [
      {
        "name": "ss-twitter",
        "title": "Twitter",
        "width": 4,
        "file": "ss-twitter.html",
        "headerColor": "#55acee"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}
